-- Usage file for various components in DSL
--	1. Initialization
--	2. Log Statement
--	3. Access Message Components
-- Run it with genericData.data

LOG "########################################Start of Rule Application for a Message########################################";

--'Variable Initialization'
--$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$

--Initializing variable
--============================================================================================================================================================
init i=2;
init j = 5;

--Block Initializing
--============================================================================================================================================================
init {
	high_limit=1020;
	low_limit=1000;
	}

--'Log' Usage
--$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$

--Logging Statements
--============================================================================================================================================================
LOG "Showing how to log";

--Logging Variables
--============================================================================================================================================================
i = i + 1 ;
LOG "i" + ":" + i ;
LOG "j:" + j scol

--Logging Current State of engine
--============================================================================================================================================================
LOG "engine:" + subtree(engine) ;

--'Access Message Components'
--$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$

--Get an individual component from existing message
--============================================================================================================================================================
LOG "--------- MSG from Asset: Engine Temp: "+ msg(engine.temp) ;
LOG "--------- MSG from Asset: Brakes Temp: "+ msg(brakes.temp) ;
